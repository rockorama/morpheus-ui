{
  "name": "@morpheus-ui/icons",
  "version": "0.0.1",
  "description": "Morpheus UI - Icon Collection",
  "module": "lib/index.js",
  "main": "lib/index.js",
  "svgr": {
    "icon": true,
    "out-dir": "components"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MainframeHQ/morpheus-ui.git"
  },
  "author": "Mainframe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MainframeHQ/morpheus-ui/issues"
  },
  "homepage": "https://github.com/MainframeHQ/morpheus-ui#readme",
  "scripts": {
    "build:clean": "rm -rf ./lib && rm -rf ./components",
    "build:components": "npx @svgr/cli -d ./components ./svgs",
    "build:js": "create-index ./components && BABEL_ENV='browser-cjs' babel components --out-dir lib --ignore test.js",
    "build": "yarn build:clean && yarn build:components && yarn build:js",
    "start": "watch 'yarn build' ./svgs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "hoist-non-react-statics": "^3.0.1",
    "lodash": "^4.17.11"
  },
  "peerDependencies": {
    "react": "^16.4.2"
  },
  "devDependencies": {
    "flow-bin": "^0.80.0"
  }
}
